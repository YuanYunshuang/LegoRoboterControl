<launch>
<param name="motor_publish_rate" value="100"/> 
<node name="nxt" pkg="nxt_control" type="nxt_control_node" output="screen">
<param name="axis_length" value="0.106"/>
<param name="wheel_radius_l" value="0.017"/>
<param name="wheel_radius_r" value="0.017"/>

<param name="tol_trans" value="0.15"/>
<param name="tol_rot" value="0.2"/>

<param name="gear_ratio_right" value="1"/>
<param name="gear_ratio_left" value="1"/>

<param name="turning_adaption" value="1.05"/>
<param name="max_turn_speed" value="80"/>

<param name="enc_per_turn_left" value="380"/>
<param name="enc_per_turn_right" value="380"/>
<param name="max_speed" value="75"/>





</node>

</launch>
